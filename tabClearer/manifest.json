{
    "manifest_version": 2,
  
    "name": "tabClearer plugin",
    "description": "This extension clears non locked tabs on click",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
    },
    "permissions": [
     "activeTab"
     ]
  }